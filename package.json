{
  "name": "hydroponics-app",
  "version": "0.0.0",
  "engines": {
    "node": "16.14.0",
    "npm": "8.3.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test": "ng test --code-coverage --source-map=false",
    "test:watch": "ng test --code-coverage --source-map=false --watch",
    "test:sourcemaps": "ng test --code-coverage --source-map=true",
    "test:sourcemaps:watch": "ng test --code-coverage --source-map=true --watch",
    "test:headless": "ng test --watch=false --code-coverage --source-map=true --browsers=ChromeHeadless",
    "serve:dev": "ng serve --live-reload --configuration dev",
    "serve:local": "ng serve --live-reload --configuration local",
    "heroku-postbuild": "ng build --configuration ${ENV}",
    "preinstall": "npx npm-force-resolutions"
  },
  "dependencies": {
    "@angular/animations": "~14.1.0",
    "@angular/cdk": "^14.1.0",
